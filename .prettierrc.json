{
  "trailingComma": "es5",
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "always",
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "plugins": [
    "prettier-plugin-jsdoc"
  ],
  "jsdocDescriptionWithDot": true,
  "jsdocVerticalAlignment": true,
  "jsdocSeparateReturnsFromParam": true,
  "jsdocPreferCodeFences": true,
  "tsdoc": true
}
